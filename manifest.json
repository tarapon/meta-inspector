{
  "manifest_version": 2,
  "name": "Meta inspector",
  "description": "Page metadata plugin for chrome devtools",
  "version": "1.0",
  "icons": {
  	"256": "icon.png"
  },
  "minimum_chrome_version": "47.0",
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
       "background.js"
    ]
  },
  "permissions": [
    "tabs", 
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ]
}